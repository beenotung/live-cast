{
  "name": "web-socket-version",
  "version": "0.0.0",
  "type": "commonjs",
  "description": "This version is used when UDP is not available.",
  "keywords": [],
  "author": "",
  "license": "BSD-2-Clause",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "start": "npm run build && ts-node-dev src/server.ts",
    "test": "tsc --noEmit",
    "clean": "rimraf dist",
    "build": "esbuild --bundle src/app.ts --outfile=public/bundle.js",
    "tsc": "tsc -p .",
    "dev": "npm run build -- --watch"
  },
  "dependencies": {
    "@beenotung/tslib": "^24.14.0",
    "express": "^4.21.2",
    "listening-on": "^2.1.0",
    "ws": "^8.18.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.5",
    "@types/node": "^24.10.0",
    "@types/ws": "^8.5.12",
    "esbuild": "^0.25.12",
    "rimraf": "^6.1.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.9.3"
  },
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ]
  }
}
